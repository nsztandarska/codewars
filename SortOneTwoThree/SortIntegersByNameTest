import org.junit.Test;
import static org.junit.Assert.assertArrayEquals;

public class SortIntegersByNameTest {

    @Test
    public void test() {
        assertArrayEquals(new int[]{8, 8, 9, 9, 10, 10}, SortIntegersByName.sort(new int[]{8, 8, 9, 9, 10, 10}));
        assertArrayEquals(new int[]{4, 1, 3, 2}, SortIntegersByName.sort(new int[]{1, 2, 3, 4}));
        assertArrayEquals(new int[]{8, 9, 0}, SortIntegersByName.sort(new int[]{0, 9, 8}));
    }
}
